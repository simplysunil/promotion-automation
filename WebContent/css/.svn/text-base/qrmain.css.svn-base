textarea {  border: 1px solid #e1e3eb;  }
input[type=checkbox], input[type=radio], input[type=checkbox]:focus, input[type=radio]:focus{ border: none;outline: none;}

/*Home Page Starts*/	
  .container {width: 100%; padding: 20px;}
  .containerPanel { background: #fff ;  }
  h1.title { padding: 1em; font-weight: 700; font-family: Verdana, Geneva, sans-serif;} 
  .col-md-4 { width: 30.333%;}.col-md-8 { width: 69.667%;}
  .container h1 {
		font-size: 1.3em;
	}
  ul.questType {
		margin: 2em 4em;
	}
	ul.questType li {
		clear: both;
		padding: 1em 0 3em;
	}
  .selectQuest { margin:2em;}
  .selectQuest li {
		display: block;
		margin: 2em 0;
		padding: 0.3em;
		box-shadow: 0 3px 3px #ddd;
	}
  .createQuest{ background: #12b0a8; color: #fff; padding-left: 30px; width: 100%; margin-top: 44px; cursor:pointer}
  .createQuestTemplate{ background: #f7f8fa; color: #17b0a6; padding-left: 30px; width: 100%; margin-top: 64px; cursor:pointer;}
  .selectQuest li.createQuestTemplate{ margin-top: 4em;}
  .footerCopy{padding: 10px; background: #f7f8fa; position:fixed; bottom: 0; width: 100%}
  .questPopup ul{padding: 0; padding-top: 20px; margin: 0 auto; width: 85%; list-style-type: none;}
  .questPopup ul li{padding-bottom: 10px;}
  .questPopup input{border: 1px solid #d9d9d9; width: 99%; padding-left: 2%;}
  .selectQuest li{ display:block; margin:2em 0; padding:1.2em;}
  .questPopup .submitQ{ color: #fff; float:right;}
  .popupBg{ background:#000; opacity: 0.5; width: 100%; height: 100%; position:fixed; opacity: 0.5; top: 0; bottom: 0; left: 0; right: 0; z-index: 999; display: none;}
  .opacit{ opacity: 0.5;}
  ::-webkit-input-placeholder { color: #000;}
  :-moz-placeholder{ color: #000; }
  ::-moz-placeholder { color: #000;  }
  :-ms-input-placeholder { color: #000;  }
	.closeBtn {
		color: #f15e6b;
		cursor: pointer;
		font-size: 1.6em;
		position: absolute;
		right: 0.3em;
		top: 0.3em;
		background: url(../images/icons/close_normal.png) no-repeat;
		width: 18px;
		height:18px;
	}
	.closeBtn:hover {background: url(../images/icons/close_hover.png) no-repeat;}
/*Home Page CSS ends*/

/*Page Header Starts*/

/* Initiate Contract upload pop up */

/* Start 29-09-2014 for DMS */

.upload_photo{margin-top:28px; text-align: center;}
th.dms_download_th, th.dms_delete_th{padding:10px 0 0 10px !important; text-align:left !important;}
.dms_download, .dms_delete{color:#666; font-weight:normal; font-size:11px; padding:15px 0 10px 0;}
.dms_delete{padding-top:28px;}
.dms_download_btn{background:url(../images/icons/Download_normal.png) no-repeat; cursor:pointer; display:block; height:18px; width:18px;}
.dms_download_btn:hover{background:url(../images/icons/Download_hover.png) no-repeat;}
.dms_delete_btn{background:url(../images/icons/deleted_normal.png) no-repeat; cursor:pointer; display:block; height:16px; width:16px;}
.dms_delete_btn:hover{background:url(../images/icons/deleted_hover.png) no-repeat;}
.dms_overlay, .brc_overlay{background:#000; display:none; height:100%; left:0; opacity:.9 !important; position:fixed; top:0; width:100%; z-index:99999;}
.dms_delete_popup, .dms_delete_popup_confirmation, .dms_upload_popup, .brc_delete_popup_confirmation{background:#fff; border-radius:10px; box-sizing:border-box; display:none; padding:20px; position:fixed; width:400px; margin:0 0 0 -200px; left:50%; top:30%; z-index:9999999;}
.dms_popup_heading{font-size:15px; font-weight:bold; float:left;}
.dms_close_popup, .dms_upload_close_popup, .brc_close_popup, .photo_delete_close_popup{background:url(../images/close_normal.png) no-repeat; cursor:pointer; float:right; margin:-12px -12px 0 0; height:18px; width:18px; position:absolute; right: 28px; top:28px; display:none}
.dms_close_popup:hover, .dms_upload_close_popup:hover, .brc_close_popup:hover{background:url(../images/close_hover.png) no-repeat;}
.dms_delete_files{max-height:100px; overflow:auto; width:100%;}
.dms_delete_files label{clear:both; font-weight:normal; display:block; margin:12px 0 0 10px;}
.dms_delete_files label input{float:left; height:auto; margin:0 12px 0 0}
.dms_delete_select_all{clear:both; display:block; margin:12px 0 5px 10px;}
.dms_delete_select_all input{float:left; height:auto; margin:0 12px 0 0}
.dms_conf_msg{margin:20px 0; text-align:center;}
.browseFile{border-radius: 5px; border: 1px solid #BEBEBE; background: #ccc; cursor: pointer; display: inline-block; height: auto; margin: 20px 0 0 0; outline: none; padding: 8px 10px;}
.addFileDisable{background: #E8E8E8; border: 1px solid #D8D8D8; color: #C2C2C2;}
.file_for_upload{max-height:150px; overflow:auto; width:100%;}
.file_for_upload label{display:block; clear:both; margin:15px 0 0 0; width:100%;}
.file_for_upload div input{font-weight:normal; float:left; height:30px; margin:10px 0 0 0; padding:5px; width:80%;}
.file_for_upload div span.remove_selected_file{background:url(../images/icons/deleted_normal.png) no-repeat; cursor:pointer; float:right; height:16px; margin:17px 10px 0 0; width:16px;}
.file_for_upload div span.remove_selected_file:hover{background:url(../images/icons/deleted_hover.png) no-repeat;}
.dms_cancel_popup_btn, .dms_upload_cancel_popup_btn, .brc_cancel_popup_btn{background:#fff; border:1px solid #11b1aa; color:#11b1aa; float:left; margin:20px 0 0 0;}
.dms_upload_btn{padding: 5px 15px; height: auto;}
.dms_delete_popup_btn, .dms_delete_conf_btn, .dms_upload_btn, .brc_delete_conf_btn{float:right; margin:20px 0 0 0;}
.data-display-body tr.odd{background:#fff;}
.data-display-body tr.even{background:#E4E4E4;}

.toolTipContainer{background:#fff; box-shadow:2px 3px 4px #D2D2D2; border:2px solid #E5E5E5; border-radius:10px; padding:10px; position:absolute; left:28px; top:-14px; width:240px;}
.toolTipContainer b{width:10px; position:absolute; display:inline-block; left:-12px; height:10px; border-top:10px solid transparent; border-bottom:10px solid transparent; border-right:10px solid #E5E5E5;}

.uploadErrorMsg {color: #f00;display: none;padding: 10px 0 0 0;}


/*Remediation edits*/
.rem_radio_buttons label {display: block !important; }
.btn_comments { color: #636e7b; float: right; padding: 8px 10px; margin: 10px 0 0; }
.btn_submit_radio { margin-top: 1em; }
.btn_comments img { margin: -2px 5px 0 0; }
.customRadio label:before { top: 4px;}
#remediationeditform li { list-style: none;}
#remediationeditform .editContactBtn {padding: 5px 10px;}
.delBtn { color: #17a2d6 !important; border-color: #17a2d6 !important;}
.delBtn:hover, .delBtn:focus { background: #18a0db; color: #fff !important; border-color: #18a0db !important; }
.file { height:0; visibility: hidden; margin-top: -14px;}
.file {visibility: hidden;}
.browseBtn{background: url(../images/attachfile.png) no-repeat; width: 12px; height: 12px; margin: 6px 0 0 5px; padding: 0 10px 0 5px; vertical-align: middle; cursor: pointer; float: left;}
.uploaded_doc { color: #17a2d6;}
.upload_doc { display: block; margin-bottom: 0.5em; }
.remve { cursor: pointer; margin-left: 15px; }
.secondLevel_label { margin-left: -23px;}
.secondLevel_label:before { display: none;}
.rem_defer, .rem_delegate, .rem_reject, .rem_declare { display: none;}
.alert-success, .tprmerrormsg { font-size: 15px !important; margin: 0px auto; text-align: center; padding: 2px 0;}
.tprmerrormsg {background-color: #dff0d8; border-color: #d6e9c6; display:block;}
#remediationUtilityDTO .tprmerrormsg, #remediationUtilityDTO .alert-success { width: 100%; }


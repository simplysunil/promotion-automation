@media(max-width:700px){
	.dataTableContainer{overflow:visible; overflow-y:visible; -ms-overflow-y:visible; padding-bottom:15px; width:auto; white-space:normal;}
	.createNewContactBtn{float:none; margin:15px 0 0 0; text-align:center;}
	#lmsTable{
		border-collapse: collapse;
		border-spacing: 0;
		border: 1px solid #bbb;
	}
	#lmsTable td, #lmsTable th {
		border-top: 1px solid #ddd;
		display:block;
		padding:10px;
	}
	#lmsTable th {
		border-top:none;
		background: #F2F2F2;
		text-align:left !important;
		width:92.3% !important;
	}
	
	table td {
		max-width: 100% !important;
	}
	
	#lmsTable th b{float:right; right:170px;}
	
	.paging_full_numbers{min-width:0;}
	.dataTables_length{display:none;}
	.expandContainer{width:100%}
	.expandContainer{width:100%}
	.expandContainer table{width:100%}
	.expandContainer table thead tr th{background:rgb(48, 91, 127) !Important; color:#fff; width:auto !important;}
	.caretRight{margin-top:-5px;}
	.addRemoveTemplate.new-business-questionnaire {top: 0}
}



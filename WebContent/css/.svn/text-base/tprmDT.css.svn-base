input[type="checkbox"]{height:auto; outline:none; opacity:0; position:relative; z-index:9999;}
input[type="checkbox"] + label{position:relative;}
input[type="checkbox"] + label:after{background:url(../images/icons/form_set.png) 0 0 #fff no-repeat; content:""; margin:0 0 0 0; display:block; height:16px; width:16px; position:absolute; left:-14px; top:-9px;}
input[type="checkbox"]:hover + label:after{background-position:-18px 0;}
input[type="checkbox"]:checked + label:after{background-position:-36px 0;}

.setupQuestContainer{float:left;width:100%}
.multiChoice_dd{float:left}
.col-md-8{width: 75.667%;min-height:600px}
.branch-info{width:100%;float:left;margin: 1em 0;}
.branch-info p{width:50%;float:left}
.questContainer{border-left:1px solid #e1e3eb}
.branching-select{color:rgb(48, 91, 127); padding-bottom: 1em;}
.col-md-4,.branch-footer .footer-right {width:24.333%;border:0}

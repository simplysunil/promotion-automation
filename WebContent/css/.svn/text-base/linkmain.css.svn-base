    padding: 1em;
    text-align: center;
}
#sectionTitle, #qt_editBox{ border:#fff 1px solid; border-radius: 5px}
#sectionTitle:hover, #qt_editBox:hover{ border:#11b1aa 1px solid;border-radius: 5px}
.sectionTitle, .qt_editBox {
    float: left;
    text-align: left;
    text-overflow: ellipsis;
    width: 130px;
	white-space: nowrap;
  	overflow: hidden
}
.icon-plus {
    color: #11b1aa;
    font-size: 1.5em;
    padding: 0 0.3em;
}

/*Dynamic Tabs*/
 .quest-tabs li {
    float: left;
    left: -2px;
    margin-top: -1px;
 }
 .blueprint {
    border: 1px solid #e1e3eb;
    position: relative;
    top: -1px;
    z-index: 0;
}
 .quest-tabs {
    border-left: 1px solid #e1e3eb;
    margin-top: 6px;
    overflow: hidden;
    position: relative;
}
	.quest-tabs .tab{border:1px solid #e1e3eb;}
.quest-tabs .tab.active {
    border-bottom: 1px solid #fff;
    position: relative;
    top: 1px;
    z-index: 1000;
}
 .tab:first-child{border-left: 1px solid #e1e3eb}
.tab {
    margin: 0.1em;
    padding: 4px 5px 5px;
    position: relative;
    top: 1px;
    z-index: 12;
}
 .tab input{border-radius:3px; border: 1px solid #11b1aa; cursor: pointer; padding-left: 3px;}
	.addSection {
		border: 1px dashed #ccc;
		cursor: pointer;
		font-style: italic;
		height: 23px;
		margin-left: 5px;
		margin-top: 4px !important;
		width: 100px;
	}

  .tab .sectionInput{ border: 1px solid #fff;
    border-radius: 5px;}
  .tab .sectionInput:hover{border-color: #11b1aa}
  .questionContainer {
		border: 1px solid #e1e3eb;
		padding: 0 !important;
		position: relative;
		top: -1px;
	}

/*Custom Form Elements Starts*/	
.customInput input[type="checkbox"], .customInput input[type="radio"] {
    display: none;
}
.customInput label {
    cursor: pointer;
    display: inline-block;
    font-size: 0.9em;
    line-height: 16px;
    padding-left: 23px !important;
    position: relative;
	float: left;
}
.customInput label:before {
    content: "";
    height: 17px;
    left: 0;
    position: absolute;
    width: 17px;
	background-image: url(../images/icons/form_set.png);
	background-repeat: no-repeat; 
}

.customCheckBox label:before{background-position: 0 0;}
.customCheckBox label:hover:before{background-position: -17px 0;}
.customCheckBox input[type="checkbox"]:checked + label:before {
	background-position: -34px 0;
}
.customRadio input[type="radio"]:checked + label:before {
	background-position: -34px -17px;
}
.customRadio label:before{background-position: 0 -17px;height: 16px;}
.customRadio label:hover:before{background-position: -17px -17px;}

/*Custom Form Elements Ends*/	

/*Questionnaire UI Components Starts*/	
.qt_fields{}
.qt_fields .customInput{ float:left;}
.qt_fields .customInput label{ top: 3px}
.qt_No {
    float: left;
    padding-top: 5px;
}
#qt_editBox, .qt_editBox{ width: 90%}
.col1, .col2 {
    margin-right: 1em;
    width: 20%;
}
.qt_field_opt{
    padding: 10px 10px 10px 45px;
	clear: both;
    display: block;
    list-style: lower-alpha outside none;
}
	.qt_field_opt .qt_ansOpt, .qt_field_opt .qt_score {
		font-style: italic;
	}
	.qt_field_opt .qt_ansOpt {
		width: 50%;
	}		
		.qt_ansOpt { margin-right: 7px}
		label.qt_ansOpt {
			margin-left: 3px;
			margin-right: 4px;
		}
		.qt_ansOpt + label{ width: 5% }
		.qt_score {width: 5%; margin-right: 10px}
	.qt_field_opt input{ float:left; margin-bottom: 1em;}
	.qt_field_opt ol {
		list-style: lower-alpha outside none;
		position: relative;
		top: 1.2em;
		width: 100% !important;
	}
		.qt_field_opt ol li * {
			position: relative;
			top: -1.2em;
		}
			.qt_field_opt button {
				background-color: transparent;
				background-image: url("../images/icons/form_set.png");
				background-repeat: no-repeat;
				border: medium none;
				display: inline-block;
				height: 17px;
				margin-right: 10px;
				top: -1.45em !important;
				width: 18px;
			}
			.qt_add{ background-position: 0 -102px}
			.qt_add:hover{background-position: -18px -102px}
			.qt_remove{background-position: 0 -119px}
			.qt_remove:hover{background-position: -18px -119px}
		.qt_field_opt input[type="radio"],.qt_field_opt input[type="checkbox"] { margin-left: -20px !important}

/* Date Picker*/
.datepicker { width: 85px; margin-right: 5px;}
.ui-datepicker-trigger{ background:url(../images/icons/Datetime.png) no-repeat; width: 18px; height:18px;}

/*Date picker UI - custom */
.ui-datepicker { background: #fff; border: #17b4ad 1px solid; width: 200px}
.ui-datepicker .ui-datepicker-header{ background: #e0f1f0} 
.ui-datepicker-title select.ui-datepicker-month{ float: left; width: auto}

/* CSS Document */
/*Common CSS*/
.fl{ float:left}
.fr{ float:right}
input {
   border: 1px solid #ccc;
    border-radius: 5px;
    font-style: normal !important;
    height: 25px;
    padding: 0 0.5em;
}
textarea {  border: 1px solid #ccc;   border-radius: 5px; }
input:focus, textarea:focus{border: 1px solid #11b1aa;}
.link { color: #428bca; cursor: pointer;}
.link:hover {color: #2a6496}
/*Home Page Starts*/	
  .container {width: 100%; padding: 20px;}
  .containerPanel { background: #fff ; border-radius: 5px; }
  h1.title { padding: 1em; font-weight: 700; font-family: Verdana, Geneva, sans-serif;} 
  .col-md-4 { width: 30.333%;}.col-md-8 { width: 69.667%;}
  .container h1 {
		font-size: 1.3em;
	}
  ul.questType {
		margin: 2em 4em;
	}
	ul.questType li {
		clear: both;
		padding: 1em 0 3em;
	}
  .selectQuest { margin:2em;}
  .selectQuest li {
		display: block;
		margin: 2em 0;
		padding: 0.3em;
		box-shadow: 0 3px 3px #ddd;
	}
  .createQuest{ background: #12b0a8; color: #fff; padding-left: 30px; border-radius: 5px; width: 100%; margin-top: 44px; cursor:pointer}
  .createQuestTemplate{ background: #f7f8fa; color: #17b0a6; padding-left: 30px; border-radius: 5px; width: 100%; margin-top: 64px; cursor:pointer;}
  .selectQuest li.createQuestTemplate{ margin-top: 4em;}
  .footerCopy{padding: 10px; background: #f7f8fa; position:fixed; bottom: 0; width: 100%}
  .questPopup {width: 340px; height: 135px; background: #fff; border-radius: 10px; position: fixed; 
   top: 50%; left: 50%; margin: -100px 0 0 -100px; z-index: 1000; display:none;}
  .questPopup ul{padding: 0; padding-top: 20px; margin: 0 auto; width: 85%; list-style-type: none;}
  .questPopup ul li{padding-bottom: 15px;}
  .questPopup input{border: 1px solid #d9d9d9; width: 99%; border-radius: 5px; color: #000 !important; 
  font-size: 11px; font-weight: bold; height: 25px; padding-left: 2%;}
  .selectQuest li{ display:block; margin:2em 0; padding:1.2em;}
  .questPopup .submitQ{ color: #fff; float:right;}
  .popupBg{ background:#000; opacity: 0.5; width: 100%; height: 100%; position:fixed; opacity: 0.5; top: 0; bottom: 0; left: 0; right: 0; z-index: 99; display: none;}
  .opacit{ opacity: 0.5;}
  ::-webkit-input-placeholder { color: #000;}
  :-moz-placeholder{ color: #000; }
  ::-moz-placeholder { color: #000;  }
  :-ms-input-placeholder { color: #000;  }
	 .closeBtn {
		color: #f15e6b;
		cursor: pointer;
		font-size: 1.6em;
		position: absolute;
		right: 0.3em;
		top: 0.3em;
		background: url(../images/icons/close_normal.png) no-repeat;
		width: 24px;
		height:24px;
	}
	.closeBtn:hover {background: url(../images/icons/close_hover.png) no-repeat;}
	.ui-autocomplete{ padding: 0 !important; width: 285px !important;}
	.fetchTemplateName{margin-bottom: 1.5em !important; }
	.fetchTemplateName:focus { border:1px solid #11b1aa; }
/*Home Page CSS ends*/

/*Page Header Starts*/
.questHeader {
    background: #f7f9fa;
     -webkit-border-top-left-radius: 15px;
	-webkit-border-top-right-radius: 15px;
	-moz-border-radius-topleft: 15px;
	-moz-border-radius-topright: 15px;
	border-top-left-radius: 15px;
    border-top-right-radius: 15px;
    padding: 0.5em;
}
	.questHeader h2 {
		float: left;
		font-size: 1.2em; 
		line-height: 2.5em;
	}
		.questHeader .questTitleEdit{ color: #11b1aa; text-decoration:underline; padding-left:0.5em}
		.questHeader .questTitleEdit:hover{ text-decoration:none}
/*Page Header End*/	

.setupQuestContainer{ background:#fff; border-top:#e1e3eb 2px solid}

/*Left Panel Section Starts*/
.panels{
	border-right:#e1e3eb 1px solid;
	min-height: 560px;
}
#tabs_container {
    border-bottom: 1px solid #ccc;
}
.panel-tabs {
    border-bottom: 1px solid #ccc;
}
.panel-tabs li {
    border-right: 1px solid #e1e3eb;
    display: inline-block;
    margin-right: 2px;
}
.panel-tabs li:last-child{ 
	 border-right: none;
}
.panel-tabs li a {
    font-size: 1em;
    line-height: 4em;
    padding-right: 5px;
    text-decoration: none;
	color:#929292;
}
.panel-tabs li a:hover {
	color:#000;
}
.panel-tabs li.active a {color:#000;}
.panel-tabs li.active a:hover {color:#000;}

.tab_content {
    display: none;
	padding:1em 0;
}

	
#questType li {
    padding: 0.25em 0;
}
#questBank li, #questTemplate li{ padding: 0.5em 0;}

#questType ul > li{ color: #000;}
#questType ul li ul {
    padding: 0.5em 0 0;
}
#questType ul li ul li {
    color: #929292;
    margin-top: 0.5em;
}
#questBank ol, #questTemplate ol{ padding-left: 2em; list-style:decimal; color:#428bca}	
#questType ul li ul li:hover{ background: #f4f5f5;
	color: #11b1aa;
	cursor: pointer;
}
	
/*Left Panel Section Ends*/
	
.questContainer{}
.dragItemLabel {
    background: none repeat scroll 0 0 #f4f5f5;
    border: 1px dashed #ccc;
    border-radius: 5px;
    font-size: 0.8em;
    font-style: italic;

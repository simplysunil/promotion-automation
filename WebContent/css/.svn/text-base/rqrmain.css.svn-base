/* CSS Document */
/*Common CSS*/
.fl {
	float: left
}

.fr {
	float: right
}

input {
	border: 1px solid #ccc;
	border-radius: 5px;
	font-style: normal !important;
	height: 25px;
	padding: 0 0.5em;
}

textarea {
	border: 1px solid #ccc;
	border-radius: 5px;
}

input:focus,textarea:focus {
	border: 1px solid #11b1aa;
}
input[type=checkbox], input[type=radio], input[type=checkbox]:focus, input[type=radio]:focus{ border: none;outline: none;}

/*Home Page Starts*/
.container {
	width: 100%;
	padding: 20px;
}

.containerPanel {
	background: #fff;
	border-radius: 5px;
}

h1.title {
	padding: 1em;
	font-weight: 700;
	font-family: Verdana, Geneva, sans-serif;
}

.col-md-4 {
	width: 30.333%;
}

.col-md-8 {
	width: 69.667%;
}

.container h1 {
	font-size: 1.3em;
}

ul.questType {
	margin: 2em 4em;
}

ul.questType li {
	clear: both;
	padding: 1em 0 3em;
}

.selectQuest {
	margin: 2em;
}

.selectQuest li {
	display: block;
	margin: 2em 0;
	padding: 0.3em;
	box-shadow: 0 3px 3px #ddd;
}

.createQuest {
	background: #12b0a8;
	color: #fff;
	padding-left: 30px;
	border-radius: 5px;
	width: 100%;
	margin-top: 44px;
	cursor: pointer
}

.createQuestTemplate {
	background: #f7f8fa;
	color: #17b0a6;
	padding-left: 30px;
	border-radius: 5px;
	width: 100%;
	margin-top: 64px;
	cursor: pointer;
}

.selectQuest li.createQuestTemplate {
	margin-top: 4em;
}

.footerCopy {
	padding: 10px;
	background: #f7f8fa;
	position: fixed;
	bottom: 0;
	width: 100%
}

.questPopup {
	width: 340px;
	background: #fff;
	position: fixed;
	z-index: 100;
	top: 50%;
	left: 50%;
	margin: -100px 0 0 -100px;
	z-index: 9999;
	display: none;
}

.questPopup ul {
	padding: 0;
	padding-top: 20px;
	margin: 0 auto;
	width: 85%;
	list-style-type: none;
}

.questPopup ul li {
	padding-bottom: 15px;
}

.questPopup input {
	border: 1px solid #d9d9d9;
	width: 99%;
	border-radius: 5px;
	color: #000 !important;
	font-size: 11px;
	height: 22px;
	padding-left: 2%;
}

.selectQuest li {
	display: block;
	margin: 2em 0;
	padding: 1.2em;
}

.questPopup .submitQ {
	color: #fff;
	float: right;
}

.popupBg {
	background: #000;
	opacity: 0.5;
	width: 100%;
	height: 100%;
	position: fixed;
	opacity: 0.5;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 999;
	display: none;
}

.opacit {
	opacity: 0.5;
}

::-webkit-input-placeholder {
	color: #000;
}

:-moz-placeholder {
	color: #000;
}

::-moz-placeholder {
	color: #000;
}

:-ms-input-placeholder {
	color: #000;
}

.closeBtn {
	color: #f15e6b;
	cursor: pointer;
	font-size: 1.6em;
	position: absolute;
	right: 0.3em;
	top: 0.3em;
}
/*Home Page CSS ends*/

/*Page Header Starts*/
.questHeader {
	background: #f7f9fa;
	-webkit-border-top-left-radius: 15px;
	-webkit-border-top-right-radius: 15px;
	-moz-border-radius-topleft: 15px;
	-moz-border-radius-topright: 15px;
	border-top-left-radius: 15px;
	border-top-right-radius: 15px;
	padding: 0.5em;
}

.questHeader h2 {
	float: left;
	font-size: 1.2em;
	line-height: 2.5em;
}

.questHeader .questTitleEdit {
	color: #11b1aa;
	text-decoration: underline;
	padding-left: 0.5em
}

.questHeader .questTitleEdit:hover {
	text-decoration: none
}
/*Page Header End*/
.setupQuestContainer {
	background: #fff;
	border-top: #e1e3eb 2px solid
}

/*Left Panel Section Starts*/
.panels {
	border-right: #e1e3eb 1px solid;
	min-height: 560px;
}

#tabs_container {
	border-bottom: 1px solid #ccc;
}

.panel-tabs {
	border-bottom: 1px solid #ccc;
}

.panel-tabs li {
	border-right: 1px solid #e1e3eb;
	display: inline-block;
	margin-right: 2px;
}

.panel-tabs li:last-child {
	border-right: none;
}

.panel-tabs li a {
	font-size: 1em;
	line-height: 4em;
	padding-right: 5px;
	text-decoration: none;
	color: #929292;
}

.panel-tabs li a:hover {
	color: #000;
}

.panel-tabs li.active a {
	color: #000;
}

.panel-tabs li.active a:hover {
	color: #000;
}

.tab_content {
	display: none;
	padding: 1em 0;
}

#questType li {
	padding: 0.25em 0;
}

#questBank li,#questTemplate li {
	padding: 0.5em 0;
}

#questType ul>li {
	color: #000;
}

#questType ul li ul {
	padding: 0.5em 0 0;
}

#questType ul li ul li {
	color: #929292;
	margin-top: 0.5em;
}

#questBank ol,#questTemplate ol {
	padding-left: 2em;
	list-style: decimal;
	color: #428bca
}

#questType ul li ul li:hover {
	background: #f4f5f5;
	color: #11b1aa;
	cursor: pointer;
}

/*Left Panel Section Ends*/
.questContainer {
	
}

.dragItemLabel {
	background: none repeat scroll 0 0 #f4f5f5;
	border: 1px dashed #ccc;
	border-radius: 5px;
	font-size: 0.8em;
	font-style: italic;
	margin: 30% 1em;
	padding: 1em;
	text-align: center;
}

#sectionTitle,#qt_editBox {
	border: #fff 1px solid;
	border-radius: 5px
}

#sectionTitle:hover,#qt_editBox:hover {
	border: #11b1aa 1px solid;
	border-radius: 5px
}

.sectionTitle,.qt_editBox {
	float: left;
	text-align: left;
	text-overflow: ellipsis;
	width: 130px;
	white-space: nowrap;
	overflow: hidden
}

.icon-plus {
	color: #11b1aa;
	font-size: 1.5em;
	padding: 0 0.3em;
}

/*Dynamic Tabs*/
.quest-tabs li {
	float: left;
	left: -2px;
	margin-top: -1px;
}

.blueprint {
	border: 1px solid #e1e3eb;
	position: relative;
	top: -1px;
	z-index: 0;
}

.quest-tabs {
	border-left: 1px solid #e1e3eb;
	margin-top: 6px;
	overflow: hidden;
	position: relative;
}

.quest-tabs .tab {
	border: 1px solid #e1e3eb;
}

.quest-tabs .tab.active {
	border-bottom: 1px solid #fff;
	position: relative;
	top: 1px;
	z-index: 99;
}

.tab:first-child {
	border-left: 1px solid #e1e3eb
}

.tab {
	margin: 0.1em;
	padding: 4px 5px 5px;
	position: relative;
	top: 1px;
	z-index: 12;
}

.tab input {
	border-radius: 3px;
	border: 1px solid #11b1aa;
	cursor: pointer;
	padding-left: 3px;
}

.addSection {
	border: 1px dashed #ccc;
	cursor: pointer;
	font-style: italic;
	height: 23px;
	margin-left: 5px;
	margin-top: 4px !important;
	width: 100px;
}

.tab .sectionInput {
	border: 1px solid #fff;
	border-radius: 5px;
}

.tab .sectionInput:hover {
	border-color: #11b1aa
}

.questionContainer {
	border: 1px solid #e1e3eb;
	padding: 0 !important;
	position: relative;
	top: -1px;
}

/*Custom Form Elements Starts*/
.customInput input[type="checkbox"],.customInput input[type="radio"] {
	display: none;
}

.customInput label {
	cursor: pointer;
	display: inline-block;
	font-size: 0.9em;
	line-height: 16px;
	padding-left: 23px !important;
	position: relative;
	float: left;
}

.customInput label:before {
	content: "";
	height: 17px;
	left: 0;
	position: absolute;
	width: 17px;
	background-image: url(../images/icons/form_set.png);
	background-repeat: no-repeat;
}

.customCheckBox label:before {
	background-position: 0 0;
}

.customCheckBox label:hover:before {
	background-position: -17px 0;
}

.customCheckBox input[type="checkbox"]:checked+label:before {
	background-position: -34px 0;
}

.customRadio input[type="radio"]:checked+label:before {
	background-position: -34px -17px;
}

.customRadio label:before {
	background-position: 0 -17px;
	height: 16px;
}

/*.customRadio label:hover:before {
	background-position: -17px -17px;
}*/

/*Custom Form Elements Ends*/

/*Questionnaire UI Components Starts*/
.qt_fields {
	
}

.qt_fields .customInput {
	float: left;
}

.qt_fields .customInput label {
	top: 3px
}

.qt_No {
	float: left;
	padding-top: 5px;
}

#qt_editBox,.qt_editBox {
	width: 90%
}

.qt_field_opt {
	padding: 10px 10px 10px 45px;
	clear: both;
	display: block;
	list-style: lower-alpha outside none;
}

.qt_field_opt .qt_ansOpt,.qt_field_opt .qt_score {
	font-style: italic;
}

.qt_field_opt .qt_ansOpt {
	width: 50%;
}

.qt_ansOpt {
	margin-right: 7px
}

label.qt_ansOpt {
	margin-left: 3px;
	margin-right: 4px;
}

.qt_ansOpt+label {
	width: 5%
}

.qt_score {
	width: 5%;
	margin-right: 10px
}

.qt_field_opt input {
	float: left;
	margin-bottom: 1em;
}

.qt_field_opt ol {
	list-style: lower-alpha outside none;
	position: relative;
	top: 1.2em;
	width: 100% !important;
}

.qt_field_opt ol li * {
	position: relative;
	top: -1.2em;
}

.qt_field_opt button {
	background-color: transparent;
	background-image: url("../images/icons/form_set.png");
	background-repeat: no-repeat;
	border: medium none;
	display: inline-block;
	height: 17px;
	margin-right: 10px;
	top: -1.45em !important;
	width: 18px;
}

.qt_add {
	background-position: 0 -102px
}

.qt_add:hover {
	background-position: -18px -102px
}

.qt_remove {
	background-position: 0 -119px
}

.qt_remove:hover {
	background-position: -18px -119px
}

.qt_field_opt input[type="radio"],.qt_field_opt input[type="checkbox"] {
	margin-left: -20px !important
}

/* Date Picker*/
.datepicker {
	width: 85px;
	margin-right: 5px;
}

.ui-datepicker-trigger {
	background: url(../images/icons/Datetime.png) no-repeat;
	width: 18px;
	height: 18px;
}

.ui-datepicker-prev,.ui-datepicker-next {
	display: none
}
/*Date picker UI - custom */
.ui-datepicker {
	background: #fff;
	border: #17b4ad 1px solid;
	width: 200px
}

.ui-datepicker .ui-datepicker-header {
	background: #e0f1f0
}

.ui-datepicker-title select.ui-datepicker-month {
	float: left;
	width: auto
}

.ui-datepicker-title select.ui-datepicker-year {
	float: right;
	width: auto
}

.ui-datepicker th {
	text-align: right
}

.ui-datepicker table *,.ui-datepicker td a {
	color: #333;
	font-family: Verdana, Geneva, sans-serif
}

.qt_boolean_Radio .qt_ansOpt {
	width: 15%;
}

.qt_multiChoice_Radio input[type="radio"] {
	margin-right: 5px;
	position: relative;
	top: 2px;
}

.qt_multiChoice_Radio ul li input[type="radio"]+label {
	width: 14%;
}

.qt_multiChoice_Radio label {
	width: 15%;
}

.qt_multi input,.qt_multi label {
	float: left
}

.qt_multi li {
	float: left;
	width: 100%;
}

.qt_multiSelect_chkbox input {
	float: left
}

.qt_ms_chkbox_txt {
	margin-right: 10px
}

/*Questionnaire UI Components Ends*/
.sectionName {
	border: #FFF 1px solid;
	border-radius: 5px
}

.sectionName:hover {
	border: #11b1aa 1px solid
}

.questContent {
	background: #fff;
}

.questContent .panelL {
	padding: 1em;
}

.questContent .panelR {
	padding: 1em;
}

/*Button CSS Starts*/
button[disabled] {
	background: #d2d3d5;
	color: #b3b3b3
}

.btn {
	background: #19b5a3;
}

.btn:hover {
	background: #42bea7;
	color: #fff
}

.btn:focus,.btn.active {
	background: #18675f;
	color: #fff
}

button.cancel {
	background: #fff;
	border: 1px solid #19b5a3;
	color: #19b5a3;
}

button.cancel:hover {
	background: #42bea7;
	color: #fff
}

button.cancel:focus {
	background: #18675f;
	color: #fff
}

button.delete {
	background: #fff;
	border: #ee848c 1px solid;
	color: #ee848c
}

button.delete:hover {
	background: #f06480;
	color: #fff
}

button.delete:focus {
	background: #973c55;
	color: #fff
}
/*Button CSS Ends*/
.questBtnGlobal button {
	margin: 0.3em
}

.qt_settings {
	background: none repeat scroll 0 0 #fff;
	padding: 0 0 0.5em 0;
}

.controlPanel {
	float: right;
}

.controlPanel li {
	display: inline-block;
	float: left;
	margin: 0.1em;
}

.controlPanel li a {
	color: #11b1aa;
	font-size: 1.4em;
	padding: 0 0.2em;
}

.dropdown-menu {
	background: none repeat scroll 0 0 #f4f5f5;
	border: medium none;
	box-shadow: none;
	margin: 5px 0 0;
	padding: 1em;
}

.bulk-answer-select {
	background: none repeat scroll 0 0 #fff;
	border: 1px solid #ccc;
	float: left;
	height: 125px !important;
	width: 100%;
}

.bulk-answer-select option {
	padding: 0.5em 0;
}

.settings-panel,.bulk-answer-panel {
	display: none;
}

.settings-panel:before,.bulk-answer-panel:before {
	-moz-border-bottom-colors: none;
	-moz-border-left-colors: none;
	-moz-border-right-colors: none;
	-moz-border-top-colors: none;
	border-color: rgba(0, 0, 0, 0) rgba(0, 0, 0, 0) #e7e7e7;
	border-image: none;
	border-style: inset inset solid;
	border-width: 6px;
	content: "";
	display: block;
	height: 0;
	position: absolute;
	right: 43px;
	top: -11px;
	width: 0;
	z-index: 99;
}

.settings-panel:before {
	right: 12px;
}

.settingsIcon {
	position: relative;
}

.settings-panel,.bulk-answer-panel {
	background: none repeat scroll 0 0 #e7e7e7;
	border-radius: 3px;
	font-size: 0.8em;
	margin: 5px 0 0;
	padding: 1em 2em;
	position: absolute;
	right: 30px;
	width: 215px;
	z-index: 100;
}

.bulk-answer-panel {
	right: 75px;
}

.bulk-answer-panel textarea {
	width: 100%;
	display: block;
	padding: 0.3em
}

.multiChoice_dd {
	width: 200px;
	position: relative
}

.multiChoice_dd_select {
	background-image: url("../images/icons/dd.png");
	background-position: 100% 0;
	background-repeat: no-repeat;
	border: 1px solid #11b1aa;
	border-radius: 5px;
	cursor: pointer;
	height: 26px;
	line-height: 2;
	padding: 0.1em 0.3em;
	position: relative;
	z-index: 2;
}

.multiChoice_dd_select.toggle {
	background-position: 100% -25px;
}

.mc_dd_opt {
	border: 1px solid #11b1aa;
	background: #fff;
	border-top: none;
	font-size: 0.8em;
	margin: 0;
	padding: 1em;
	position: absolute;
	border-bottom-right-radius: 5px;
	border-bottom-left-radius: 5px;
	width: 200px;
	z-index: 3;
}

.mc_dd_opt input {
	border: 1px solid #ccc;
	border-radius: 5px;
	font-style: normal !important;
	padding: 0 0.5em;
	margin-bottom: 1em;
}

.mc_dd_opt input:hover {
	border: 1px solid #11b1aa;
}

.mc_dd_opt .bulk,.mc_dd_opt label.bulk {
	float: left;
	width: 75%;
	font-style: italic;
}

.mc_dd_opt .score,.mc_dd_opt label.score {
	float: right;
	width: 20%;
	font-style: italic;
}

.ms-dropdown .mc_dd_opt input[type="checkbox"] {
	margin: 0 10px 0 0 !important
}

.ms-dropdown .mc_dd_opt label.bulk {
	margin-left: 23px
}

.ms-dropdown .mc_dd_opt .bulk {
	width: 63%
}

.settings-panel>.row {
	margin-bottom: 15px;
}

.settings-panel .col-md-7 {
	text-align: right;
}

#tag {
	font-size: 1.2em;
	font-weight: bold;
	padding: 3px;
}

.settings-btn {
	border-top: 1px solid #ccc;
	padding-top: 1em;
}

.btngroup {
	float: left;
	padding-top: 1em;
	width: 100%;
}

.btn {
	background: none repeat scroll 0 0 #11b1aa;
}

.bootstrap-switch.bootstrap-switch-large {
	min-width: 40px;
}

.bootstrap-switch {
	border: medium none;
}

.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-handle-on,.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-handle-off,.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-label
	{
	font-size: 1em;
}

.bootstrap-switch-container,.bootstrap-switch .bootstrap-switch-handle-on.bootstrap-switch-primary,.bootstrap-switch .bootstrap-switch-handle-off.bootstrap-switch-primary,.bootstrap-switch .bootstrap-switch-handle-on.bootstrap-switch-default,.bootstrap-switch .bootstrap-switch-handle-off.bootstrap-switch-default
	{
	background: none repeat scroll 0 0 #11b1aa;
}

.bootstrap-switch .bootstrap-switch-label {
	background: none repeat scroll 0 0 #fff;
	padding: 2px;
	width: 20.333%;
}

.bootstrap-switch.bootstrap-switch-on .bootstrap-switch-label,.bootstrap-switch.bootstrap-switch-off .bootstrap-switch-label,.bootstrap-switch .bootstrap-switch-handle-on,.bootstrap-switch .bootstrap-switch-container,.bootstrap-switch
	{
	border-radius: 25px;
}

.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-handle-on,.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-handle-off,.bootstrap-switch.bootstrap-switch-large .bootstrap-switch-label
	{
	color: #fff;
	padding: 0;
}

.bootstrap-switch .bootstrap-switch-label {
	margin: 3px 0 !important;
}

.bootstrap-switch .bootstrap-switch-handle-on,.bootstrap-switch .bootstrap-switch-handle-off
	{
	width: 39.333%;
}

.bootstrap-switch .bootstrap-switch-container {
	width: 150%;
}

.bootstrap-switch.bootstrap-switch-focused {
	box-shadow: none;
}

.selectPanel {
	cursor: move;
}

.sortable>div {
	border-bottom: 1px solid #ccc;
	margin: 0 0.5em;
	padding: 0.5em;
}

.ui-sortable-helper {
	border: 1px dashed #ccc;
	opacity: 0.8;
}

.delete {
	color: #f00;
}

button.save,button.save-bulkanswer {
	float: right;
}

.btn {
	color: #ffffff;
}

a:hover {
	cursor: pointer;
}

.red {
	color: #ff0000;
}

.questionContainer input[type="checkbox"] {
	border: 1px solid #11b1aa;
}

/*Icons List*/
*:before,*:after {
	box-sizing: border-box;
}

*:before,*:after {
	box-sizing: border-box;
}

/*ICON set*/
.questTypeSprite ul li i {
	background-image: url("../images/icons/question_type_sprite.png");
	background-repeat: no-repeat;
	display: inline-block;
	height: 16px;
	margin-right: 0.7em;
	position: relative;
	top: 3px;
	width: 16px;
}

.icon-sTxtBox {
	background-position: 0 0;
}

.questTypeSprite ul li:hover .icon-sTxtBox {
	background-position: -18px 0;
}

.icon-mTxtBox {
	background-position: 0 -16px;
}

.questTypeSprite ul li:hover .icon-mTxtBox {
	background-position: -18px -16px;
}

.icon-radio {
	background-position: 0 -33px;
}

.questTypeSprite ul li:hover .icon-radio {
	background-position: -18px -33px;
}

.icon-dropdown {
	background-position: 0 -50px;
}

.questTypeSprite ul li:hover .icon-dropdown {
	background-position: -18px -50px;
}

.icon-checkBox {
	background-position: 0 -67px;
}

.questTypeSprite ul li:hover .icon-checkBox {
	background-position: -18px -67px;
}

.icon-dateSelection {
	background-position: 0 -84px;
}

.questTypeSprite ul li:hover .icon-dateSelection {
	background-position: -18px -84px;
}

.ui-datepicker-trigger {
	background-image: url("../images/icons/question_type_sprite.png")
		!important;
	background-repeat: no-repeat;
	background-position: 0 -101px;
}

.icon-datePicker {
	background-position: 0 -101px;
}

.questTypeSprite ul li:hover .icon-datePicker,.ui-datepicker-trigger:hover,.ui-datepicker-trigger:active
	{
	background-position: -18px -101px;
}

.icon-dateTime {
	background-position: 0 -118px;
}

.questTypeSprite ul li:hover .icon-dateTime {
	background-position: -17px -118px;
}

.qt_each_settings {
	background-image: url("../images/icons/form_set.png");
	background-repeat: no-repeat;
	display: inline-block;
	height: 18px;
	position: relative;
	top: 4px;
	width: 18px;
}

.icon-sort {
	background-position: 0 -135px;
}

.icon-sort:hover {
	background-position: -17px -135px;
}

.icon-settings {
	background-position: 0 -153px;
}

.icon-settings:hover {
	background-position: -18px -153px;
}

.icon-delete {
	background-position: 0 -172px;
}

.icon-delete:hover {
	background-position: -18px -172px;
}

.icon-dd {
	display: inline-block;
	width: 18px;
	height: 18px;
	background: #000;
}

/*Comments*/
.comments-tab {
	margin: 0 0 1em 0;
}

.comments-tab li {
	float: left;
	border-right: #ccc 1px solid !important;
	position: relative;
	top: 1px;
	margin: 0;
}

.comments-tab li.active {
	border-bottom: #fff 1px solid;
	background: #fff;
}

.tpCommentName {
	color: #a1386d;
}

.comments-tab li a {
	background: none repeat scroll 0 0 #f4f4f4;
	font-size: 1.3em;
	padding: 1em 2em;
}

.comments-tab li.active a {
	color: #17b3ab;
	background: #fff;
}

.comments-tab_content {
	padding: 2em;
}

.search-recepients {
	background-image: url("../images/icons/comments.png");
	background-position: 107% -78px;
	background-repeat: no-repeat;
	float: right;
	padding: 0 2em 0 0.5em;
	width: 300px;
}

.search-recepients:focus {
	background-position: 107% -98px
}

/*.ui-helper-hidden-accessible {
	display: none
}*/

.ui-autocomplete {
	border: #17b3ab 1px solid;
	border-top: none;
	padding: 0.5em;
	width: 300px;
	background: #fff;
	border-bottom-left-radius: 5px;
	border-bottom-right-radius: 5px;
}

.ui-autocomplete li {
	display: block;
	padding: 0.5em 0;
}

.ui-autocomplete li a {
	color: #929292
}

.ui-autocomplete li a:hover {
	text-decoration: none;
	color: #17b3ab
}

.comments-txt {
	padding: 3em 0;
}

.comments-txt textarea,.comments-txt input {
	display: block;
	width: 100%;
}

.send-TP {
	color: #17b3ab;
	font-size: 1.2em;
	float: left
}

.comments-txt textarea {
	margin: 1em 0;
	min-height: 5em;
	padding: 0.3em;
}

.comments-txt button {
	width: 5em;
	float: right
}

.toRecepient {
	background: #fff;
}

.commentsTails li {
	border: #f4f4f4 1px solid;
	background: #f4f4f4;
	margin-bottom: 1em;
}

.commentsTails li .btn-reply {
	margin: 0.5em 0.5em 0.5em 0;
}

.comments-reply {
	float: left;
	margin: 1em 0;
	width: 100%;
}

.comments-reply textarea {
	width: 100%;
	min-height: 7em;
	padding: 0.4em;
}

.comments-reply .btn-post {
	float: right;
	margin-top: 1em;
	width: 5em;
}

.textArea {
	position: relative
}

.textArea textarea {
	resize: none;
	padding-right: 16px;
}

.comments-attach {
	background: url("../images/icons/comments.png") no-repeat scroll 0
		-133px;
	height: 21px;
	position: absolute;
	right: 3px;
	top: 3px;
	width: 13px;
}

.comments-attach:hover {
	background-position: -13px -133px;
	height: 21px;
	position: absolute;
	right: 3px;
	top: 3px;
	width: 13px;
}

.comments-trail-outer {
	background: #fff;
	height: 200px;
	overflow-y: scroll;
	width: 100%;
}

.comments-trail {
	padding: 2em;
}

.comments-trail h1 {
	display: block;
}

.comments-trail h1 .icon-toggle {
	background: url(../images/icons/comments.png) no-repeat 0 0;
	cursor: pointer;
	width: 16px;
	height: 16px;
	float: right;
	display: inline-block;
}

.comments-trail h1 .icon-toggle:hover {
	background-position: 0 -19px
}

.comments-trail h1 .active {
	background-position: 0 -36px
}

.comments-trail h1 .active:hover {
	background-position: 0 -60px
}

.comments-trail li {
	display: block;
	width: 100%;
}

.comments-trail-container {
	float: left;
	padding: 1em 1em 1em 80px;
	position: relative;
	width: 100%;
	background: #fff;
}

.btn-reply {
	float: right;
	margin-bottom: 1em;
	width: 5em;
}

.comments-image {
	left: 1em;
	padding-right: 0.5em;
	position: absolute;
}

.comments-details {
	
}

.rName {
	float: left;
	font-size: 1.2em;
}

.rName-detail {
	display: none
}

.rName-detail a {
	color: #636e7b;
	text-decoration: none;
	padding-left: 0.5em
}

.date-time {
	float: right
}

.comments-desc {
	padding: 1em 0;
	color: #333;
	word-break: break-all;
	word-wrap: break-word;
}

.btn-recepient {
	background: #19b5a3;
	border: none;
	color: #fff
}

.btn-recepient:hover {
	background: #42bea7;
	color: #fff
}

.btn-recepient.toggle {
	background: #18675f;
	color: #fff
}

.icon-comments {
	background: url(../images/icons/comments.png);
	background-repeat: no-repeat;
}

.icon-recepient {
	background-position: 0 -121px;
	display: inline-block;
	height: 10px;
	margin-right: 0.2em;
	width: 15px;
}

.btn-recepient.toggle .icon-recepient {
	background-position: -15px -121px;
}

/*Login*/
/*Password*/
.forgotDiv .input {
	padding-top: 0.5em !important;
}

.redTxt {
	color: #F00
}

.forgotDiv {
	border-top: #ccc 1px solid;
	padding: 1em;
}

.pwd-fields div {
	padding: 1em;
}

.pwd-fields .col-md-4 {
	text-align: right
}

.forgot-ans {
	width: 90%
}

/*Notifications CSS*/
.icon-tick {
	background: url("../images/icons/resetpassword_tick.png") no-repeat
		scroll 0 0 rgba(0, 0, 0, 0);
	display: inline-block;
	height: 18px;
	margin-left: 0.3em;
	margin-top: 1px;
	width: 18px;
}

.icon-wrong {
	background: url("../images/icons/resetpassword_wrong.png") no-repeat
		scroll 0 0 rgba(0, 0, 0, 0);
	display: inline-block;
	height: 18px;
	margin-left: 0.3em;
	margin-top: 1px;
	width: 18px;
}

.status-info {
	background: url("../images/icons/notify_info.png") no-repeat scroll 10px
		center #d9eff9;
	border: 1px solid #26a8e0;
	color: #0d7182;
	display: block;
	padding: 1em 1em 1em 3em;
}

.status-success {
	background: url("../images/icons/notify_successmessage.png") no-repeat
		scroll 10px center #dbebbc;
	border: 1px solid #618338;
	color: #05683a;
	display: block;
	padding: 1em 1em 1em 3em;
}

.status-warn {
	background: url("../images/icons/notify_warning.png") no-repeat scroll
		10px center #facfcf;
	border: 1px solid #b11f23;
	color: #b11f23;
	display: block;
	padding: 1em 1em 1em 3em;
}

/*User Management*/
.org-form,.um-form {
	padding: 2em 0
}

.org-form input,.org-form .customSelect,.org-form textarea {
	float: left;
	margin-bottom: 1em;
	width: 50%;
	resize: none;
}

.org-form textarea {
	min-height: 80px
}

.org-form label {
	text-align: right;
	padding: 0.5em 1em;
	float: left;
	font-weight: normal;
	margin-bottom: 1em;
	width: 40%;
}

.org-form label.desc {
	width: 20%
}

.org-form textarea {
	width: 75%;
	margin-bottom: 1em;
}

.addElement,.removeElement,.aElement,.rElement,.addLinkElement,.removeLinkElement {
	background-image: url("../images/icons/form_set.png");
	background-repeat: no-repeat;
	cursor: pointer;
	display: inline-block;
	height: 17px;
	margin: 5px;
	width: 18px;
}

.addElement,.aElement,.addLinkElement {
	background-position: 0 -102px
}

.addElement:hover,.aElement:hover {
	background-position: -18px -102px
}

.removeElement,.rElement,.removeLinkElement  {
	background-position: 0 -119px
}


.addElement, .removeElement, .aElement, .rElement, .addPhone, .removePhone,.addLinkElement, .removeLinkElement {
    background-image: url("../images/icons/form_set.png");
    background-repeat: no-repeat;
    cursor: pointer;
    display: inline-block;
    height: 17px;
    margin: 5px;
    width: 18px;
 }  

.removeElement:hover,.rElement:hover {
	background-position: -18px -119px

}
.addElement, .aElement, .addPhone, .addLinkElement{ background-position: 0 -102px}
.addElement:hover, .aElement:hover. .addPhone:hover {background-position: -18px -102px}
.removeElement, .rElement, .removePhone, .removeLinkElement {background-position: 0 -119px}
.removeElement:hover, .rElement:hover, .removePhone:hover {background-position: -18px -119px}

